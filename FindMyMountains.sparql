# retrieve altitude:

PREFIX ont:  <http://dbpedia.org/ontology/>
select distinct ?mountain ?altitude {
   ?mountain rdf:type ont:Mountain  .
   OPTIONAL {
      ?mountain ont:elevation ?altitude
   }
}

